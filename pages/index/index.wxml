<view class="like"><van-nav-bar
  custom-style = "background:rgb(121, 187, 231);"
  title-class = "color: rgb(248, 29, 0);" 
  title="個人"
/></view>
<view class="view_contain">
  <!-- 第一部分 -->
  <view class="view_1">
    <view class="view_image_text">
      <view class="userinfo-avatar" bindtap="bindViewTap">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <view class="view_3">
    <van-cell title="個人案件" is-link bind:tap="JumpCase" />
    <van-cell title=" パスワード変更" is-link bind:tap="userUpdate" />
  </view>
</view>
<!-- 底部导航栏 -->
<footer-nav-bar id="footer-nav-bar-id"
    navList="{{footerNavList}}"
    bind:onFooterNavChange="onFooterNavChange"
>
</footer-nav-bar>